{
  "name": "chatbot",
  "version": "1.0.0",
  "description": "Chatbot application with frontend and backend",
  "scripts": {
    "dev": "concurrently \"cd frontend && set PORT=3000 && npm run start-frontend\" \"cd backend && npm start\"",
    "start-frontend": "cd frontend && npm run start-frontend",
    "start-backend": "cd backend && npm start",
    "build-frontend": "cd frontend && npm run build",
    "build-backend": "cd backend && npm run build"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "keywords": [
    "chatbot",
    "react",
    "express",
    "postgresql"
  ],
  "author": "",
  "license": "MIT"
}
